{
  "version": 2,
  "installCommand": "npm install",
  "buildCommand": "npx prisma generate --schema=./prisma/schema.prisma",
  "framework": null,
  "functions": {
    "api/index.ts": {
      "memory": 1024,
      "maxDuration": 30
    }
  },
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/index.ts" },
    { "source": "/(.*)", "destination": "/api/index.ts" }
  ]
}
